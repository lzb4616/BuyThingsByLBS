<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bishe.buythingsbylbs"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:logo="@drawable/ic_launcher"
        android:theme="@style/AppTheme"
        android:name="com.bishe.MyApplication"
        
         >
        <activity
            android:name="com.bishe.ui.activity.MainActivity"
            android:icon="@drawable/ic_launcher"
            android:launchMode="singleTask"
            android:label="@string/app_name"
             >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bishe.ui.activity.TestActivity"></activity>
        <activity android:name="com.bishe.ui.activity.LoginAndRegisterActivity" android:launchMode="singleTask"></activity>
        <activity android:launchMode="singleTask" android:name="com.bishe.ui.activity.PublishThingsActivity"></activity>
    </application>

</manifest>